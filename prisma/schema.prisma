
generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["relationJoins"]
}

datasource db {
  provider  = "postgresql"
  url  	    = env("DATABASE_URL")
}

model Comment {
  id  String @id @default(cuid())
  name  String
  content String 
  createdAt DateTime @default(now())
}



